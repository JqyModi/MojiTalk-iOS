ALWAYS_SEARCH_USER_PATHS = YES
CLANG_CXX_LANGUAGE_STANDARD = c++14
CLANG_CXX_LIBRARY = libc++
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/MojiLive2D
EXCLUDED_ARCHS[sdk=iphonesimulator*] = arm64
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_TARGET_SRCROOT}/Core/include" "${PODS_TARGET_SRCROOT}/Framework/src" "${PODS_TARGET_SRCROOT}/Bridge" "${PODS_TARGET_SRCROOT}/MotionSync/Framework/src" "${PODS_TARGET_SRCROOT}/MotionSync/Core/include"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/../Libs/Live2D/Core/lib/ios" "${PODS_ROOT}/../Libs/Live2D/MotionSync/Core/CRI/lib/iOS/Release-iphoneos"
OTHER_LDFLAGS = $(inherited) -l"Live2DCubismCore_fat" -l"Live2DCubismMotionSyncEngine_CRI" -l"c++" -framework "AVFoundation" -framework "AudioToolbox" -framework "CoreGraphics" -framework "Metal" -framework "MetalKit" -framework "QuartzCore"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_DEVELOPMENT_LANGUAGE = ${DEVELOPMENT_LANGUAGE}
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/../Libs/Live2D
PODS_XCFRAMEWORKS_BUILD_DIR = $(PODS_CONFIGURATION_BUILD_DIR)/XCFrameworkIntermediates
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_HEADERMAP = NO
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
